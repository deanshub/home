*.home.shubapp.com{
  tls {
    dns godaddy {env.GODADDY_TOKEN}
  }

  @test host test.home.shubapp.com 
  handle @test {
    respond "Hello, world!"
  }

  @ha host ha.home.shubapp.com
  handle @ha {
    reverse_proxy localhost:8123
  }

  @caddy host caddy.home.shubapp.com
  handle @caddy {
    reverse_proxy localhost:2019
  }
}
